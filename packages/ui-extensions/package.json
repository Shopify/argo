{
  "name": "@shopify/ui-extensions",
  "version": "2.0.0-alpha.0",
  "main": "index.js",
  "module": "index.mjs",
  "esnext": "index.esnext",
  "types": "./build/typescript/index.d.ts",
  "typesVersions": {
    "*": {
      "admin": [
        "./build/ts/surfaces/admin/index.d.ts"
      ],
      "checkout": [
        "./build/ts/surfaces/checkout/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "esnext": "./index.esnext",
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./admin": {
      "esnext": "./admin.esnext",
      "import": "./admin.mjs",
      "require": "./admin.js"
    },
    "./checkout": {
      "esnext": "./checkout.esnext",
      "import": "./checkout.mjs",
      "require": "./checkout.js"
    }
  },
  "license": "MIT",
  "sideEffects": false,
  "dependencies": {
    "@remote-ui/core": "^2.1.16"
  },
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org/"
  }
}
