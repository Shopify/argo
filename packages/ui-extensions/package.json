{
  "name": "@shopify/ui-extensions",
  "version": "2022.10.5",
  "main": "index.js",
  "module": "index.mjs",
  "esnext": "index.esnext",
  "types": "./build/ts/index.d.ts",
  "typesVersions": {
    "*": {
      "checkout": [
        "./build/ts/surfaces/checkout.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./build/ts/index.d.ts",
      "esnext": "./index.esnext",
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./checkout": {
      "types": "./build/ts/surfaces/checkout.d.ts",
      "esnext": "./build/esnext/surfaces/checkout.esnext",
      "import": "./build/esm/surfaces/checkout.mjs",
      "require": "./build/cjs/surfaces/checkout.js"
    }
  },
  "license": "MIT",
  "sideEffects": false,
  "dependencies": {
    "@remote-ui/async-subscription": "^2.1.12",
    "@remote-ui/core": "2.1.x"
  },
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org/"
  },
  "files": [
    "build",
    "src",
    "index.*",
    "checkout.*",
    "README.md"
  ],
  "eslintConfig": {
    "rules": {
      "@shopify/strict-component-boundaries": "off"
    }
  }
}
