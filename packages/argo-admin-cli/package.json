{
  "name": "@shopify/argo-admin-cli",
  "version": "0.8.3-alpha.3",
  "description": "CLI commands to help with developing with Argo",
  "author": "Shopify <app-extensions@shopify.com>",
  "homepage": "https://github.com/Shopify/argo-admin#readme",
  "license": "MIT",
  "main": "cli.js",
  "scripts": {
    "build": "tsc --build tsconfig.prod.json && cp -r ./src/server/host ./server/host",
    "check": "prettier --check \"**/src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"**/src/**/*.{ts,tsx}\"",
    "lint": "eslint ./src --ext .ts,.tsx --format codeframe",
    "server": "SHOPIFY_DEV=true ts-node ./src/cli server --entry \"sample-extension.tsx\" --port=39351 --type=\"PRODUCT_SUBSCRIPTION\" --shop=shop1.myshopify.io --apiKey=subscriptions-dev-key",
    "test-build": "SHOPIFY_DEV=true ts-node ./src/cli build --entry \"sample-extension.tsx\""
  },
  "bin": {
    "argo-admin-cli": "./cli.js"
  },
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.1",
    "@babel/plugin-proposal-numeric-separator": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.0",
    "@babel/preset-react": "^7.10.0",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/runtime": "^7.10.1",
    "@remote-ui/react": "^2.0.6",
    "@remote-ui/web-workers": "^1.3.0",
    "@shopify/argo-admin": "0.8.3-alpha.3",
    "@shopify/argo-admin-host": "0.8.3-alpha.3",
    "@shopify/css-utilities": "^1.0.6",
    "@shopify/polaris": "^5.10.1",
    "@shopify/polaris-icons": "^4.1.0",
    "@svgr/webpack": "^5.4.0",
    "@types/dotenv-webpack": "^1.8.0",
    "@types/koa-webpack": "^6.0.0",
    "@types/webpack-dev-server": "^3.11.0",
    "@types/yargs": "^15.0.4",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "chalk": "^4.0.0",
    "cors": "^2.8.5",
    "css-loader": "^3.4.2",
    "dotenv-webpack": "^1.8.0",
    "file-loader": "^6.0.0",
    "get-port": "^5.1.1",
    "graphql-tag": "^2.10.4",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.15",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "style-loader": "^1.1.3",
    "typescript": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@shopify/argo-admin-react": "0.8.3-alpha.3",
    "@shopify/eslint-plugin": "^37.0.1",
    "eslint": "^7.6.0",
    "prettier": "^2.0.5",
    "ts-node": "^9.0.0"
  }
}
