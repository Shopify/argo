{
  "name": "@shopify/admin-ui-extensions",
  "version": "1.1.0",
  "main": "index.js",
  "module": "index.mjs",
  "esnext": "index.esnext",
  "types": "./build/ts/index.d.ts",
  "typesVersions": {
    "*": {
      "extension-api": [
        "./build/ts/extension-api/index.d.ts"
      ],
      "extension-points": [
        "./build/ts/extension-points/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./build/ts/index.d.ts",
      "esnext": "./index.esnext",
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./*": "./*",
    "./extension-api": {
      "types": "./build/ts/extension-api/index.d.ts",
      "esnext": "./extension-api.esnext",
      "import": "./extension-api.mjs",
      "require": "./extension-api.js"
    },
    "./extension-points": {
      "types": "./build/ts/extension-points/index.d.ts",
      "esnext": "./extension-points.esnext",
      "import": "./extension-points.mjs",
      "require": "./extension-points.js"
    }
  },
  "license": "MIT",
  "sideEffects": false,
  "dependencies": {
    "@remote-ui/core": "2.1.x"
  },
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org/"
  },
  "scripts": {
